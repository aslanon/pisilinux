/* #QSlider */

    QSlider::groove:horizontal {
        height: 12px;
        background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0,
            stop:0 rgba(255, 255, 255, 20));
        margin: 2px 0;
    }

    QSlider::handle:horizontal {
        background: qlineargradient(spread:repeat, x1:0, y1:0, x2:0, y2:1,
            stop:0.42623 rgba(200, 79, 136, 255),
            stop:0.489247 rgba(200, 52, 119, 255),
            stop:1 rgba(200, 0, 105, 255));
        border: 1px solid #5c5c5c;
        width: 18px;
        margin: 0 0;
        border-radius: 2px;
    }
    
    QSlider::handle:horizontal:hover {
        border:1px solid  rgba(34, 49, 60, 100);
        width: 18px;
        margin: 0 0;
        border-radius: 2px;
        background: qlineargradient(spread:repeat, x1:0, y1:0, x2:0, y2:1,
           stop:0.42623 rgba(200, 79, 136, 200),
           stop:0.489247 rgba(200, 52, 119, 200),
           stop:1 rgba(200, 0, 105, 200));
    }
    

