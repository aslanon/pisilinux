/* #QComboBox */

    QComboBox {
        border:1px solid  rgba(255, 255, 255, 30);
        border-radius: 2px;
        font-size: 13px;
        color:white
    }

    QComboBox:editable {
        border:0px;
        border-bottom: 1px solid rgba(255, 255, 255, 30);
        background-color: rgba(255, 255, 255, 200);
    }

    QComboBox:!editable, QComboBox::drop-down:editable {
        background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0,
                                         stop:0 rgba(255, 255, 255, 10));
    }

    /* QComboBox gets the "on" state when the popup is open */
    QComboBox:!editable:on{
        background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0,
            stop:0 rgba(255, 255, 255, 255));
    }

    QComboBox::drop-down:editable:on {
        background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0,
            stop:0 rgba(255, 255, 255, 255));    
    }
    
    QComboBox:disabled {
        background-color: rgba(160, 160, 160, 160);
    }

    QComboBox:!editable:hover, QComboBox::drop-down:editable:hover {
        border:1px solid  rgba(255, 255, 255, 60);
        border-radius:2px;
        background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0,
            stop:0 rgba(255, 255, 255, 15));

    }

    QComboBox:on:hover {
        border:1px solid  rgba(255, 255, 255, 40);
        background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0,
            stop:0 rgba(255, 255, 255, 15));
    }

    QComboBox:on { /* shift the text when the popup opens */
        padding-top: 3px;
        padding-left: 4px;
        border:0px;
        border-bottom: 1px solid rgba(255, 255, 255, 30);
        background-color: rgba(255, 255, 255, 200);        
    }

    QComboBox::drop-down {
        subcontrol-origin: padding;
        subcontrol-position: top right;
        width: 22px;
        border-top-right-radius: 4px;
        border-bottom-right-radius: 3px;
    }

    QComboBox::drop-down:on {
        subcontrol-origin: padding;
        subcontrol-position: top right;
        width: 22px;
        border-top-right-radius: 4px;
        border-bottom-right-radius: 3px;
    }

    QComboBox::down-arrow {
        image: url(/usr/share/icons/crystalsvg/16x16/actions/1downarrow.png);
    }

    QComboBox::down-arrow:on { /* shift the arrow when popup is open */
        top: 1px;
        left: 1px;
    }

    QComboBox::down-arrow {
        right: 0px;
        image: url(:/images/arrow-down.png);
    }

    QComboBox::down-arrow:on {
        top: 1px;
    }

    QComboBox QAbstractItemView {
        color: #222222;
        border:0px;
        border-bottom: 1px solid rgba(255, 255, 255, 30);
        background-color: rgba(255, 255, 255, 200);
    }


/* #QLineEdit */

    QLineEdit {
        background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0,
            stop:0 rgba(255, 255, 255, 10));
        color: white;
        border:1px solid  rgba(255, 255, 255, 40);
        border-radius: 3px;
        font-size: 15px
    }

    QLineEdit:hover {
        background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0,
            stop:0 rgba(255, 255, 255, 15));
        color: white;
        border: 1px solid gray;
        border:1px solid  rgba(255, 255, 255, 60);
        border-radius: 3px;
    }

    QLineEdit:disabled {
        border: 1px solid gray;
        border-radius: 2px
    }


/* #QTextEdit */

    QTextEdit {
        border:1px solid  rgba(255, 255, 255, 30);
        border-radius: 4px;
        background-color: rgba(0, 0, 0, 0);
        selection-background-color: gray;
    }


/* #QTreeView */

    QTreeView {
        border: 1px solid rgba(255,255,255,80);
        border-radius: 3px;
        selection-background-color: rgba(255, 255, 255, 30);
        background-color:rgba(255, 255, 255, 10);
        color:white
    }

    QTreeView::branch:has-children:!has-siblings:closed,
    QTreeView::branch:closed:has-children:has-siblings {
        border-image: none;
        image: url(:/gui/pics/expand.png);;
    }

    QTreeView::branch:open:has-children:!has-siblings,
    QTreeView::branch:open:has-children:has-siblings  {
        border-image: none;
        image: url(:/gui/pics/collapse.png);;
    }


/* #QHeaderView Table Column Headers */

    QHeaderView::section {
        background-color:qlineargradient(spread:repeat, x1:0, y1:0, x2:0, y2:1,
           stop:0.42623 rgba(200, 79, 136, 200), 
           stop:0.489247 rgba(200, 52, 119, 200),
           stop:1 rgba(200, 0, 105, 200));
        font-size: 8pt;
        color: white;
        padding-left:5px;
        border-right: 1px solid rgba(255,255,255,20);
        border-left: 1px solid rgba(255,255,255,20);
        border-top: 1px solid rgba(255,255,255,20);
        border-bottom: 1px solid rgba(255,255,255,80);
        height: 27px
    }

    /* style the sort indicator */
    QHeaderView::down-arrow {
        image: url(:/images/arrow-down.png);
    }

    QHeaderView::up-arrow {
        image: url(:/images/arrow-up.png);
    }


/* #QProgressBar */        

    QProgressBar {
        border-radius: 2px;
        text-align: center;
        color: #222222;
        background-color: rgba(255, 255, 255, 180);
        border:1px solid qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.276201, fx:0.5, fy:0.5,
            stop:0 rgba(255, 255, 255, 100),
            stop:1 rgba(0, 0, 0, 0))
    }

    QProgressBar:chunk {
        background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
            stop:0.473118 rgba(200, 79, 136, 255),
            stop:0.489247 rgba(200, 52, 119, 255),
            stop:1 rgba(200, 0, 105, 255));
        border-radius: 2px;
    }
    
/* #QScrollArea */

    QScrollArea::corner {
            background-color: rgba(255, 255, 255, 0);
            border: 0px;
    }

    QScrollBar:vertical {
            width: 13px;
            margin: 3px 3px 3px 3px;
            background-color: rgba(255, 255, 255, 0);
    }

    QScrollBar::handle:vertical {
            min-height: 16px;
            background-color: rgba(0,0,0, 150);
            border: 1px solid rgb(150, 150, 150);
            border-radius: 3px;
            padding:3px;
    }

    QScrollBar::handle:vertical:hover {
            min-height: 16px;
            background-color: rgba(50,50,50, 150);
            border-radius: 3px;
    }

    QScrollBar::add-line:vertical {
            border: 0px;
            height: 0px;
            subcontrol-position: bottom;
            subcontrol-origin: margin;
            background: transparent;
    }

    QScrollBar::sub-line:vertical {
            border: 0px;
            height: 0px;
            subcontrol-position: top;
            subcontrol-origin: margin;
            background: transparent;
    }

    QScrollBar:horizontal {
            height: 13px;
            margin: 3px 3px 3px 3px;
            background: transparent;
    }

    QScrollBar::handle:horizontal {
            min-width: 16px;
            background-color: rgba(0,0,0, 150);
            border: 1px solid rgb(150, 150, 150);
            border-radius: 3px;
    }

    QScrollBar::handle:horizontal:hover {
            min-width: 16px;
            background-color: rgba(50,50,50, 150);
            border-radius: 3px;
    }

    QScrollBar::add-line:horizontal {
            border: none;
            background: transparent;
            width: 0px;
            subcontrol-position: right;
            subcontrol-origin: margin;
    }

    QScrollBar::sub-line:horizontal {
            border: none;
            width: 0px;
            subcontrol-position: left;
            subcontrol-origin: margin;
    }
